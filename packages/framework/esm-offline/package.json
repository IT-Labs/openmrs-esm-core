{
  "name": "@openmrs/esm-offline",
  "version": "5.1.0",
  "license": "MPL-2.0",
  "description": "Helper utilities for OpenMRS",
  "module": "dist/openmrs-esm-offline.js",
  "types": "dist/index.d.ts",
  "source": true,
  "scripts": {
    "test": "jest --config jest.config.js --passWithNoTests",
    "build": "rollup -c --environment NODE_ENV:production",
    "build:development": "rollup -c",
    "analyze": "rollup -c --environment NODE_ENV:production,ANALYZE:true",
    "typescript": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx"
  },
  "keywords": [
    "openmrs",
    "microfrontends"
  ],
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-core.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-core/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-core#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@openmrs/esm-api": "5.x",
    "@openmrs/esm-globals": "5.x",
    "@openmrs/esm-state": "5.x",
    "rxjs": "6.x"
  },
  "dependencies": {
    "dexie": "^3.0.3",
    "lodash-es": "^4.17.21",
    "uuid": "^9.0.0",
    "workbox-window": "^6.1.5"
  },
  "devDependencies": {
    "@openmrs/esm-api": "^5.1.0",
    "@openmrs/esm-globals": "^5.1.0",
    "@openmrs/esm-state": "^5.1.0",
    "@types/uuid": "^9.0.1",
    "rollup": "^3.27.2",
    "rxjs": "^6.5.3"
  }
}
